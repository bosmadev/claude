  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "playwriter",
    "context7",
    "serena"
  ],
  "hooks": {
    "Setup": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\token-guard.py check --quiet",
            "timeout": 60
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\setup.py validate-symlinks",
            "timeout": 30
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\ralph.py stop",
            "timeout": 30
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ~/.claudeCodeChange\\tools\\claudeChangeStop.js",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "startup|resume",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\utils.py model-capture",
            "timeout": 5
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\ralph.py session-start",
            "timeout": 10
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\ralph.py pre-compact",
            "timeout": 10
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Read",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\auto-allow.py",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\security-gate.py pre-bash",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\git.py pre-commit-checks",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "MultiEdit|Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\auto-allow.py",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "node ~/.claudeCodeChange\\tools\\claudeChangePreToolUse.js",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\ralph.py hook-pretool",
            "timeout": 10
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\git.py command-history",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\git.py change-tracker",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\guards.py guardian",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\guards.py plan-write-check",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\guards.py insights-reminder",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "ExitPlanMode",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\guards.py ralph-enforcer",
            "timeout": 10
          }
        ]
      },
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\ralph.py agent-tracker",
            "timeout": 10
          }
        ]
      },
      {
        "matcher": "Skill",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\guards.py skill-validator",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Skill",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\post-review.py hook",
            "timeout": 30
          }
        ]
      }
    ],
    "UserPromptSubmit": [
           {
             "matcher": "^/(?!start\\b)",
          "hooks": [
                {
              "type": "command",
               "command": "python ~/.claude\\scripts\\guards.py skill-interceptor",
              "timeout": 5
             }
             ]
          },
         {
            "matcher": "^/start",
             "hooks": [
              {
              "type": "command",
          "command": "python ~/.claude\\scripts\\guards.py skill-parser",
               "timeout": 5
            }
           ]
       },
         {
           "hooks": [
           {
              "type": "command",
            "command": "python ~/.claude\\scripts\\guards.py plan-comments",
             "timeout": 5
             },
             {
              "type": "command",
              "command": "python ~/.claude\\scripts\\guards.py auto-ralph",
              "timeout": 5
             }
           ]
          }
      ],
    "SubagentStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\ralph.py hook-subagent-start",
            "timeout": 10
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\scripts\\ralph.py hook-subagent-stop",
            "timeout": 10
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "permission_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "python ~/.claude\\hooks\\utils.py notify",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "python ~/.claude\\scripts\\statusline.py"
  },
  "enabledPlugins": {},
  "outputStyle": "Engineer",
  "spinnerVerbs": {
    "mode": "replace",
    "verbs": [
      "Unifying"
    ]
  },
  "autoUpdatesChannel": "latest"
}
